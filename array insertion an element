#include <stdio.h>

int main()
{
    int arr[10],x,i,n,pos;
    printf("enter the number of elemenet ");
    scanf("%d",&n);
    
    print("enter the element");
    for(i=0;i<=n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("entered array is :");
     for(i=0;i<=n;i++)
    {
        printf("%d\n",arr[i]);
    }
    printf("enter  insert element");
    scanf("%d",&x);
    printf("enter  the  position of searched element");
    scanf("%d",&pos);
    n=n+1;
     for(i=n-1;i>=pos;i--)
    {
      arr[i]=arr[i-1];
      
    }
    arr[pos]=x;
    print("new array");
     for(i=0;i<=n;i++)
    {
        printf("%d\n",arr[i]);
    }
    return 0;
}
